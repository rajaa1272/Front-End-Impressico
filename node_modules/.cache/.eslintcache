n                ]\n            }\n            break;\n\n        case \"DELITEM\":\n            const exist1 = state.find((x) => x.id === product.id);\n            if (exist1.qty === 1) {\n                return state.filter((x) => x.id !== exist1.id);\n            }\n            else {\n                return state.map((x) => x.id === product.id ? { ...x, qty: x.qty - 1 } : x);\n            }\n            break;\n\n        default:\n            return state;\n            break;\n    }\n}\n\nexport default handleCart;","/home/arepellinagaraj/Downloads/e-mart-2/Shopify/src/redux/action/index.js",[],[],"/home/arepellinagaraj/Downloads/e-mart-2/Shopify/src/data.js",[],[],"/home/arepellinagaraj/Downloads/e-mart-2/Shopify/src/component/Contact.jsx",["134","135","136","137","138","139","140","141","142"],[],"/home/arepellinagaraj/Downloads/e-mart-2/Shopify/src/component/content_option.jsx",[],[],"/home/arepellinagaraj/Downloads/e-mart-2/Shopify/src/component/AddProduct.jsx",[],[],{"ruleId":"143","severity":1,"message":"144","line":11,"column":11,"nodeType":"145","messageId":"146","endLine":11,"endColumn":19},{"ruleId":"147","severity":1,"message":"148","line":30,"column":8,"nodeType":"149","endLine":30,"endColumn":10,"suggestions":"150"},{"ruleId":"143","severity":1,"message":"151","line":4,"column":19,"nodeType":"145","messageId":"146","endLine":4,"endColumn":23},{"ruleId":"147","severity":1,"message":"152","line":25,"column":36,"nodeType":"153","endLine":25,"endColumn":41},{"ruleId":"143","severity":1,"message":"144","line":8,"column":11,"nodeType":"145","messageId":"146","endLine":8,"endColumn":19},{"ruleId":"154","severity":1,"message":"155","line":17,"column":5,"nodeType":"156","endLine":17,"endColumn":14},{"ruleId":"143","severity":1,"message":"157","line":3,"column":10,"nodeType":"145","messageId":"146","endLine":3,"endColumn":17},{"ruleId":"143","severity":1,"message":"158","line":3,"column":19,"nodeType":"145","messageId":"146","endLine":3,"endColumn":26},{"ruleI